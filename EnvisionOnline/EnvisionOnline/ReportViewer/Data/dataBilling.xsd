          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="RPT_BillingTableAdapter" GeneratorDataComponentClassName="RPT_BillingTableAdapter" Name="RPT_Billing" UserDataComponentName="RPT_BillingTableAdapter">
