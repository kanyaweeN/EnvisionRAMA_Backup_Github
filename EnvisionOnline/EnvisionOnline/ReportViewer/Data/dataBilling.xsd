              <DbSource ConnectionRef="RIS_RAMAConnectionString (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT ord.ORG_ID,
		ord.ORDER_ID,
		ord.ORDER_DT,
		dbo.fnGetEmpThaiName(ord.CREATED_BY) as RegisterBy,
		ord.LMP_DT,
		dbo.fnGetPatientStatus(CONVERT(int, ISNULL(ord.PAT_STATUS, 0))) as PatientStatus,
		dbo.fnGetUnit(ord.REF_UNIT)+' ['+dbo.fnGetUnitUID(ord.REF_UNIT)+']' as OrderDept,
		insurance.INSURANCE_TYPE_DESC,
		reg.HN,
		case when reg.GENDER='F' then N'ญ' 
		     when reg.GENDER='M' then N'ช' 
		     else N'อื่นๆ' end as [GENDER],
		RTRIM(ISNULL(reg.TITLE, '') + ' ' + ISNULL(reg.FNAME, '') + ' ' + ISNULL(reg.LNAME, '')) as PatientName,
		RTRIM(ISNULL(reg.FNAME_ENG, '') + ' ' + ISNULL(reg.LNAME_ENG, '')) as PatientNameEng,
		dbo.fnGetAge(reg.DOB, 1) as Age,
		ords.ACCESSION_NO,
		ords.QTY,
		ords.RATE,
		ords.QTY * ords.RATE as TOTAL,
		ords.LAST_MODIFIED_ON,
		exam.EXAM_UID + ' ' + exam.EXAM_NAME+' '+dbo.fnGetBPVIEW(ords.BPVIEW_ID) as ExamName,
		clinic.CLINIC_TYPE_TEXT,
		modality.MODALITY_NAME,
				case when exam.UNIT_ID = 1 then
					N'* บัตรนี้ใช้เป็นหลักฐานในการตรวจรังสีวินิจฉัย *' + char(10) + char(13)+char(10) + char(13)+
					N'  นำบัตรนี้ไปยื่นที่แผนกเดิมเพื่อรอพบแพทย์' + char(10) + char(13)+char(10) + char(13)
			 when exam.UNIT_ID = 2 then
					N'* บัตรนี้เป็นหลักฐานได้รับการตรวจรังสีวินิจฉัย *' + char(10) + char(13)+char(10) + char(13)+
					N'  นำบัตรนี้ไปยื่นที่แผนกเดิมเพื่อรอพบแพทย์ '
			 when exam.UNIT_ID = 3 then
					N'' 
			end as HR_UNIT_INS  ,
		PRIORITY = 
				CASE PRIORITY
					WHEN 'R' THEN 'Routine'
					WHEN 'U' THEN 'Urgent'
					WHEN 'S' THEN 'Stat'
					ELSE 'Routine'
				END
	FROM
		RIS_ORDER ord
		inner join RIS_ORDERDTL ords
				on ord.ORDER_ID = ords.ORDER_ID
			inner join RIS_EXAM exam
					on ords.EXAM_ID = exam.EXAM_ID
			inner join RIS_CLINICTYPE clinic
					on ords.CLINIC_TYPE = clinic.CLINIC_TYPE_ID
			inner join RIS_MODALITY modality
					on ords.MODALITY_ID = modality.MODALITY_ID
		inner join HIS_REGISTRATION reg
				on ord.REG_ID = reg.REG_ID
		left join RIS_INSURANCETYPE insurance
				on ord.INSURANCE_TYPE_ID = insurance.INSURANCE_TYPE_ID</CommandText>
