        <CommandText>/* select by datetime */
declare @tb_date table (monthly datetime)

declare @date_begin_temp datetime
set @date_begin_temp = CONVERT(datetime, left(CONVERT(nvarchar(10), @date_begin, 112), 6) + N'01')

declare @count int
declare @index int
set @count = DATEDIFF(MONTH, @date_begin, @date_end)
set @index= 0


while (@count &gt;= @index)
begin
	insert into @tb_date values (DATEADD(MONTH, @index, @date_begin_temp))

	set @index = @index + 1
end		


select [MonthText],[YearText],PivotTable.MODALITY_ID,[Month],
Unknow,Chest,Abdomen,Pelvis,skull,HeadAndNeck,Spine,UpperExtremity,LowerExtremity,Other
from( select RIS_BODYPART.BP_DESC, RIS_ORDERDTL.EXAM_DT,RIS_ORDERDTL.MODALITY_ID
 , a.monthly as [Month],
 case MONTH(a.monthly)
	when 1 then N'January'
	when 2 then N'February'
	when 3 then N'March'
	when 4 then N'April'
	when 5 then N'May'
	when 6 then N'June'
	when 7 then N'July'
	when 8 then N'August'
	when 9 then N'September'
	when 10 then N'October'
	when 11 then N'November'
	when 12 then N'December'
	else N''
end  as [MonthText],YEAR(a.monthly) as [YearText]
 
		from RIS_ORDERDTL 
			inner join RIS_EXAM on RIS_EXAM.EXAM_ID=RIS_ORDERDTL.EXAM_ID
			inner join RIS_BODYPART on RIS_BODYPART.BP_ID=RIS_EXAM.BP_ID
			inner join RIS_MODALITY on RIS_MODALITY.MODALITY_ID=RIS_ORDERDTL.MODALITY_ID
			right join @tb_date a on CONVERT(datetime, left(CONVERT(nvarchar(10), RIS_ORDERDTL.EXAM_DT, 112), 6) + N'01') = a.monthly
			and RIS_ORDERDTL.EXAM_DT between @date_begin and @date_end  and RIS_ORDERDTL.MODALITY_ID in (@MODALITY_ID)

			
) as monthly
PIVOT
(
COUNT(monthly.EXAM_DT)  
for monthly.BP_DESC IN (Unknow,Chest,Abdomen,Pelvis,skull,HeadAndNeck,Spine,UpperExtremity,LowerExtremity,Other)
)as PivotTable
order by [Month];</CommandText>
