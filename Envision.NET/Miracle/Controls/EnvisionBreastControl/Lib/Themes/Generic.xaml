    <ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:EnvisionBreastControl.Lib"
    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <!-- Static Resources -->
    <Style TargetType="{x:Type TextBox}" x:Key="NormalTextBox">
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="Margin" Value="5,0"/>
        <Setter Property="TextAlignment" Value="Center"/>
    </Style>
    <Style TargetType="{x:Type TextBlock}" x:Key="NormalTextBlock">
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="HorizontalAlignment" Value="Left"/>
        <Setter Property="Margin" Value="5,0"/>
    </Style>
    <Style TargetType="{x:Type Image}" x:Key="IconImage">
        <Setter Property="Width" Value="16"/>
        <Setter Property="Height" Value="16"/>
    </Style>
    <Style TargetType="{x:Type Image}" x:Key="SpIconImage">
        <Setter Property="Width" Value="13"/>
        <Setter Property="Height" Value="13"/>
    </Style>
    <!-- Control Template -->
    <!-- Control Expander Diameter-->
    <Style TargetType="{x:Type local:CExpandDiameterControl}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:CExpandDiameterControl}">
                    <Border Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                        <Grid>
                            <Canvas x:Name="root" VerticalAlignment="Center" HorizontalAlignment="Center"
                                    DataContext="{TemplateBinding Offset}">
                                <ContentPresenter x:Name="Content"
                                                  Width="{TemplateBinding Diameter}"
                                       Height="{TemplateBinding Diameter}"
                                       Margin="5"
                                       Canvas.Left="{Binding X1}" 
                                       Canvas.Top="{Binding Y1}"
                                       Content="{TemplateBinding ContentItem}"/>
                                <Rectangle x:Name="bounder"
                                       Width="{TemplateBinding BounderDiameter}"
                                       Height="{TemplateBinding BounderDiameter}"
                                       Stroke="{TemplateBinding LineScaleColor}" 
                                       Margin="4"
                                       StrokeThickness="{TemplateBinding StrokeThickness}"
                                       Canvas.Left="{Binding X1}" 
                                       Canvas.Top="{Binding Y1}"
                                       Visibility="{TemplateBinding ShowBounder}">
                                </Rectangle>
                                <Rectangle x:Name="MouseOverBounder"
                                       Width="{TemplateBinding BounderDiameter}"
                                       Height="{TemplateBinding BounderDiameter}"
                                       Stroke="{TemplateBinding BounderMouseOverColor}" 
                                       Margin="4"
                                       StrokeThickness="{TemplateBinding StrokeThickness}"
                                       Canvas.Left="{Binding X1}" 
                                       Canvas.Top="{Binding Y1}"
                                       Visibility="{TemplateBinding ShowMouseOverBounder}">
                                </Rectangle>
                                <Canvas x:Name="expanderCanvas" Visibility="{TemplateBinding CanExpand}">
                                    <local:CScaleRulerControl x:Name="CS_TOP" 
                                                       Canvas.Top="{Binding SR_TOP_Y}" 
                                                       Canvas.Left="{Binding SR_TOP_X}" 
                                                       LineWidth="{TemplateBinding Diameter}" 
                                                       LineColor="{TemplateBinding LineScaleColor}"
                                                       LineTextBackground="{TemplateBinding LineScaleTextBackground}"
                                                       LineThickness="{TemplateBinding LineThickness}"
                                                       FontSize="{TemplateBinding LineScaleTextFontSize}"
                                                       FontFamily="{TemplateBinding LineScaleTextFontFamily}"
                                                       LineTextWidth="{TemplateBinding LineScaleTextWidth}"
                                                       LineTextAlignment="{TemplateBinding LineScaleTextAlginment}"
                                                       Foreground="{TemplateBinding LineScaleTextForeground}"
                                                       Visibility="{TemplateBinding ShowTopScaleRuler}"
                                                       Unit="{TemplateBinding Unit}"/>
                                    <local:CScaleRulerControl x:Name="CS_BOTTOM" 
                                                       Canvas.Top="{Binding SR_BOTTOM_Y}" 
                                                       Canvas.Left="{Binding SR_BOTTOM_X}" 
                                                       LineWidth="{TemplateBinding Diameter}" 
                                                       LineColor="{TemplateBinding LineScaleColor}"
                                                       LineTextBackground="{TemplateBinding LineScaleTextBackground}"
                                                       LineThickness="{TemplateBinding LineThickness}"
                                                       FontSize="{TemplateBinding LineScaleTextFontSize}"
                                                       FontFamily="{TemplateBinding LineScaleTextFontFamily}"
                                                       LineTextWidth="{TemplateBinding LineScaleTextWidth}"
                                                       LineTextAlignment="{TemplateBinding LineScaleTextAlginment}"
                                                       LineTextOnFloatAlignment="Bottom"
                                                       Foreground="{TemplateBinding LineScaleTextForeground}"
                                                       Visibility="{TemplateBinding ShowBottomScaleRuler}"
                                                       Unit="{TemplateBinding Unit}"/>
                                    <local:CScaleRulerControl x:Name="CS_LEFT" 
                                                       Canvas.Top="{Binding SR_LEFT_Y}" 
                                                       Canvas.Left="{Binding SR_LEFT_X}" 
                                                       LineWidth="{TemplateBinding Diameter}" 
                                                       LineColor="{TemplateBinding LineScaleColor}"
                                                       LineTextBackground="{TemplateBinding LineScaleTextBackground}"
                                                       LineThickness="{TemplateBinding LineThickness}"
                                                       FontSize="{TemplateBinding LineScaleTextFontSize}"
                                                       FontFamily="{TemplateBinding LineScaleTextFontFamily}"
                                                       LineTextWidth="{TemplateBinding LineScaleTextWidth}"
                                                       LineTextAlignment="{TemplateBinding LineScaleTextAlginment}"
                                                       Foreground="{TemplateBinding LineScaleTextForeground}"
                                                       Visibility="{TemplateBinding ShowLeftScaleRuler}"
                                                       Unit="{TemplateBinding Unit}">
                                        <local:CScaleRulerControl.RenderTransform>
                                            <TransformGroup>
                                                <RotateTransform Angle="-90"/>
                                            </TransformGroup>
                                        </local:CScaleRulerControl.RenderTransform>
                                    </local:CScaleRulerControl>
                                    <local:CScaleRulerControl x:Name="CS_RIGHT" 
                                                   Canvas.Top="{Binding SR_RIGHT_Y}" 
                                                   Canvas.Left="{Binding SR_RIGHT_X}" 
                                                   LineWidth="{TemplateBinding Diameter}" 
                                                   LineColor="{TemplateBinding LineScaleColor}"
                                                   LineTextBackground="{TemplateBinding LineScaleTextBackground}"
                                                   LineThickness="{TemplateBinding LineThickness}"
                                                   FontSize="{TemplateBinding LineScaleTextFontSize}"
                                                   FontFamily="{TemplateBinding LineScaleTextFontFamily}"
                                                   LineTextWidth="{TemplateBinding LineScaleTextWidth}"
                                                   LineTextAlignment="{TemplateBinding LineScaleTextAlginment}"
                                                   Foreground="{TemplateBinding LineScaleTextForeground}"
                                                   Visibility="{TemplateBinding ShowRightScaleRuler}"
                                                   Unit="{TemplateBinding Unit}">
                                        <local:CScaleRulerControl.RenderTransform>
                                            <TransformGroup>
                                                <RotateTransform Angle="90"/>
                                            </TransformGroup>
                                        </local:CScaleRulerControl.RenderTransform>
                                    </local:CScaleRulerControl>
                                    <Rectangle x:Name="btn1" 
                                           Width="5" 
                                           Height="5" 
                                           Fill="Transparent" 
                                           StrokeThickness="{TemplateBinding StrokeThickness}" 
                                           Stroke="{TemplateBinding LineScaleColor}" 
                                           Canvas.Left="{Binding X1}" 
                                           Canvas.Top="{Binding Y1}" 
                                           Cursor="SizeNWSE" />
                                    <Rectangle x:Name="btn2" 
                                           Width="5" 
                                           Height="5" 
                                           Fill="Transparent" 
                                           StrokeThickness="{TemplateBinding StrokeThickness}" 
                                           Stroke="{TemplateBinding LineScaleColor}" 
                                           Canvas.Left="{Binding X2}" 
                                           Canvas.Top="{Binding Y2}" 
                                           Cursor="SizeNESW"  />
                                    <Rectangle x:Name="btn3" 
                                           Width="5" 
                                           Height="5" 
                                           Fill="Transparent" 
                                           StrokeThickness="{TemplateBinding StrokeThickness}" 
                                           Stroke="{TemplateBinding LineScaleColor}" 
                                           Canvas.Left="{Binding X3}" 
                                           Canvas.Top="{Binding Y3}" 
                                           Cursor="SizeNESW" />
                                    <Rectangle x:Name="btn4"
                                           Width="5" 
                                           Height="5" 
                                           Fill="Transparent" 
                                           StrokeThickness="{TemplateBinding StrokeThickness}" 
                                           Stroke="{TemplateBinding LineScaleColor}" 
                                           Canvas.Left="{Binding X4}" 
                                           Canvas.Top="{Binding Y4}" 
                                           Cursor="SizeNWSE" />
                                </Canvas>
                            </Canvas>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!-- Control Diameter Scale Ruler-->
    <Style TargetType="{x:Type local:CScaleRulerControl}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:CScaleRulerControl}">
                    <Border Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                        <Canvas x:Name="root" DataContext="{TemplateBinding LineLocation}">
                            <Line x:Name="L_AXIS" 
                                  X1="{Binding AXIS_X1}" 
                                  Y1="{Binding AXIS_Y1}" 
                                  X2="{Binding AXIS_X2}" 
                                  Y2="{Binding AXIS_Y2}" 
                                  StrokeThickness="{TemplateBinding LineThickness}" 
                                  Stroke="{TemplateBinding LineColor}"
                                  Height="1"/>
                            <Line x:Name="L_SCALE_START" 
                                  X1="{Binding SCALE_X1}" 
                                  Y1="{Binding SCALE_Y1}" 
                                  X2="{Binding SCALE_X2}" 
                                  Y2="{Binding SCALE_Y2}" 
                                  StrokeThickness="{TemplateBinding LineThickness}" 
                                  Stroke="{TemplateBinding LineColor}"
                                  Width="{Binding SCALE_W}" 
                                  Height="{Binding SCALE_H}"
                                  Canvas.Top="{Binding SCALE_OFFSET_Y}"/>
                            <Line x:Name="L_SCALE_END" 
                                  X1="{Binding SCALE_X1}" 
                                  Y1="{Binding SCALE_Y1}" 
                                  X2="{Binding SCALE_X2}" 
                                  Y2="{Binding SCALE_Y2}" 
                                  StrokeThickness="{TemplateBinding LineThickness}" 
                                  Stroke="{TemplateBinding LineColor}"
                                  Width="{Binding SCALE_W}" 
                                  Height="{Binding SCALE_H}"
                                  Canvas.Top="{Binding SCALE_OFFSET_Y}"
                                  Canvas.Left="{Binding SCALE_OFFSET_X}"/>
                            <TextBlock x:Name="TXT_DIAMETER" 
                                       Text="{Binding TEXT}" 
                                       Canvas.Left="{Binding TEXT_X}" 
                                       Canvas.Top="{Binding TEXT_Y}" 
                                       Background="{TemplateBinding LineTextBackground}"
                                       Foreground="{TemplateBinding Foreground}"
                                       Width="{TemplateBinding LineTextWidth}" 
                                       FontSize="{TemplateBinding FontSize}"
                                       FontFamily="{TemplateBinding FontFamily}"
                                       TextAlignment="{TemplateBinding LineTextAlignment}"
                                       Visibility="{TemplateBinding ShowScaleText}"/>
                        </Canvas>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
